{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "rootDir": "./src",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {},
    "resolveJsonModule": true,
    "outDir": "./dist",
    // "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "skipLibCheck": true
  }
}
